{
  "name": "support",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  },
  "nodemonConfig": {
    "ignore": [
      "dist-front/*"
    ],
    "delay": 2500
  },
  "scripts": {
    "start": "rollup -c && npm run serve",
    "serve": "sbf serve --path ./dist --port 4500",
    "serve:8080": "node bin/http-server --path ./dist --port 4500 --proxy http://localhost:8080",
    "test": "jest  --runTestsByPath test/normal.test.ts",
    "bundle": "rollup -c ",
    "bundle:dev": "cp -r environments/environment.dev.ts environments/environment.ts  && npm run bundle ",
    "bundle:prod": "cp -r environments/environment.prod.ts environments/environment.ts && npm run bundle ",
    "bundle:watch": "rollup -c -w ",
    "bundle:watch:dev": "cp -r environments/environment.dev.ts environments/environment.ts  && npm run bundle:watch",
    "bundle:watch:prod": "cp -r environments/environment.prod.ts environments/environment.ts && npm run bundle:watch",
    "serve:watch:all": "sbf serve --path ./dist --port 4500 --bundle rollup --watch",
    "serve:front": "sbf serve --path ./dist-front --port 4500",
    "serve:front:watch": "sbf serve --path ./dist-front --port 4500 --watch",
    "serve:server": "node --trace-warnings --unhandled-rejections=warn dist-server/bundle",
    "serve:server:watch": "nodemon --trace-warnings --unhandled-rejections=warn dist-server/bundle",
    "bundle:front:dev": "cp -r front/environments/environment.dev.ts front/environments/environment.ts && npm run bundle:front",
    "bundle:front:prod": "cp -r front/environments/environment.prod.ts front/environments/environment.ts && npm run bundle:front",
    "bundle:front": "rollup -c rollup.front.config.js",
    "bundle:front:watch": "rollup -c rollup.front.config.js -w ",
    "bundle:server:dev": "cp -r server/environments/environment.dev.ts server/environments/environment.ts && npm run bundle:server",
    "bundle:server:prod": "cp -r server/environments/environment.prod.ts server/environments/environment.ts && npm run bundle:server",
    "bundle:server": "rollup -c rollup.server.config.js",
    "bundle:server:watch": "rollup -c rollup.server.config.js -w "
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^26.0.24",
    "@types/jsdom": "^16.2.13",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^16.11.10",
    "@types/node-schedule": "^1.3.2",
    "@types/nodemailer": "^6.4.4",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.15",
    "rollup": "^3.26.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-import-css": "^3.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-watch": "^4.3.1",
    "simple-boot-front-cli": "^1.0.8",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "jsdom": "^18.1.1",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "node-schedule": "^2.1.0",
    "nodemailer": "^6.7.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.7",
    "simple-boot-http-ssr": "^1.0.4",
    "tslib": "^1.14.1",
    "typeorm": "^0.2.41",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  }
}
